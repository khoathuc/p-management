generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model Task {
    id String @id @default(cuid())

    name String

    //data
    data Json?

    //timestamps
    createdAt DateTime @default(now())
    updatedAt DateTime @default(now())
}
